  "dependencies": {
    "@ahrefs/bs-atdgen-codec-runtime": "^2.0.1",
    "bs-fetch": "^0.6.2",
    "react": "17.0.1",
    "react-dom": "^17.0.1",
    "reason-react": "^0.9.1",
    "remotedata-re": "^1.1.0"
  },
  "devDependencies": {
    "bs-platform": "^8.2.0",
    "parcel": "^1.12.4"
  }
}
